	JP NAAM + 5		; JP NAAM + 5
	JP 5 + NAAM		; JP 5 + NAAM

	JP NZ, NAAM + 5 	; JP NZ, NAAM + 5
	JP Z,  NAAM + 5 	; JP Z,  NAAM + 5
	JP NC, NAAM + 5 	; JP NC, NAAM + 5
	JP C,  NAAM + 5 	; JP C,  NAAM + 5
	JP PO, NAAM + 5 	; JP PO, NAAM + 5
