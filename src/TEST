	JR 5 + NAAM		; JR 5 + NAAM
	JR NZ, NAAM + 5 	; JR NZ, NAAM + 5
	JR Z,  NAAM + 5 	; JR Z,  NAAM + 5
	JR NC, NAAM + 5 	; JR NC, NAAM + 5
	JR C,  NAAM + 5 	; JR C,  NAAM + 5

	CALL NAAM + 5		; CALL NAAM + 5
	CALL NZ,NAAM + 5	; CALL NZ,NAAM + 5
	CALL Z,NAAM + 5 	; CALL Z,NAAM + 5
	CALL NC,NAAM + 5	; CALL NC,NAAM + 5
	CALL C,NAAM + 5 	; CALL C,NAAM + 5
	CALL PO,NAAM + 5	; CALL PO,NAAM + 5
	CALL PE,NAAM + 5	; CALL PE,NAAM + 5
	CALL P,NAAM + 5 	; CALL P,NAAM + 5
	CALL M,NAAM + 5 	; CALL M,NAAM + 5

	RET			; RET
	RET NZ			; RET NZ
	RET Z			; RET Z
	RET NC			; RET NC
	RET C			; RET C
	RET PO			; RET PO
	RET PE			; RET PE
	RET P			; RET P
	RET M			; RET M


	RETI			; RETI
	RETN			; RETN

;	END

	IN A,(NAAM + 5) 	; IN A,(NAAM + 5)
	IN F,(C)		; IN F,(C)
	IN A,(C)		; IN A,(C)
	IN B,(C)		; IN B,(C)
	IN C,(C)		; IN C,(C)
	IN D,(C)		; IN D,(C)
	IN E,(C)		; IN E,(C)
	IN H,(C)		; IN H,(C)
	IN L,(C)		; IN L,(C)

	OUT (NAAM * NAAM ),A	; OUT (NAAM * NAAM ),A
	OUT (C),A		; OUT (C),A
	OUT (C),B		; OUT (C),B
	OUT (C),C		; OUT (C),C
	OUT (C),D		; OUT (C),D
	OUT (C),E		; OUT (C),E
	OUT (C),H		; OUT (C),H
	OUT (C),L		; OUT (C),L

	INI			; INI
	INIR			; INIR
	IND			; IND
	INDR			; INDR
	OUTI			; OUTI
	OTIR			; OTIR
	OUTD			; OUTD
	OTDR			; OTDR

	JP (HL)
